LoadPlugin interface
<Plugin "interface">
{% if collectd_interfaces is defined %}
{% for if in collectd_interfaces %}
	Interface "{{ if }}"
{% endfor %}
{% else %}
	Interface "lo" 
{% endif %}

{% if collectd_interface_IgnoreSelected is defined %}
	IgnoreSelected "{{ collectd_interface_IgnoreSelected|default('true') }}"
{% endif %}
</Plugin>
